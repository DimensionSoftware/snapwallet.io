{
  "swagger": "2.0",
  "info": {
    "title": "api.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "Flux"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/flux/auth/one-time-passcode": {
      "post": {
        "summary": "Post email or phone in exchange for a one time passcode",
        "description": "Will cause your email or phone to receive a one time passcode.\nThis can be used in the verify step to obtain a token for login",
        "operationId": "Flux_OneTimePasscode",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/OneTimePasscodeResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/OneTimePasscodeRequest"
            }
          }
        ],
        "tags": [
          "Flux"
        ]
      }
    },
    "/flux/auth/one-time-passcode-verify": {
      "post": {
        "summary": "Post one time passcode in exchange for an access token",
        "description": "The passcode received in either email or phone text message should be provided here in order to obtain on access token",
        "operationId": "Flux_OneTimePasscodeVerify",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/OneTimePasscodeVerifyResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/OneTimePasscodeVerifyRequest"
            }
          }
        ],
        "tags": [
          "Flux"
        ]
      }
    },
    "/flux/pricing-data": {
      "get": {
        "summary": "Get pricing data",
        "description": "Provides pricing data for all markets with rate maps",
        "operationId": "Flux_PricingData",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/PricingDataResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "Flux"
        ]
      }
    },
    "/plaid/connect-bank-accounts": {
      "post": {
        "summary": "Post chosen bank info from plaid in order to create a new ACH pyment method in wyre",
        "description": "requires a plaid processor token which in turn requires a plaid widget interaction where the user selects the account id",
        "operationId": "Flux_PlaidConnectBankAccounts",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/PlaidConnectBankAccountsResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/PlaidConnectBankAccountsRequest"
            }
          }
        ],
        "tags": [
          "Flux"
        ]
      }
    },
    "/plaid/create-link-token": {
      "post": {
        "summary": "https://plaid.com/docs/link/link-token-migration-guide/",
        "operationId": "Flux_PlaidCreateLinkToken",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/PlaidCreateLinkTokenResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/PlaidCreateLinkTokenRequest"
            }
          }
        ],
        "tags": [
          "Flux"
        ]
      }
    },
    "/viewer": {
      "get": {
        "summary": "Get viewer data",
        "description": "Provides user (viewer) data associated with the access token",
        "operationId": "Flux_ViewerData",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/ViewerDataResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "Flux"
        ]
      }
    },
    "/wyre/create-acount": {
      "post": {
        "summary": "https://plaid.com/docs/link/link-token-migration-guide/",
        "operationId": "Flux_WyreCreateAccount",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/WyreCreateAccountResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/WyreCreateAccountRequest"
            }
          }
        ],
        "tags": [
          "Flux"
        ]
      }
    }
  },
  "definitions": {
    "Address": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "street1": {
          "type": "string"
        },
        "street2": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "postalCode": {
          "type": "string"
        },
        "country": {
          "type": "string"
        }
      },
      "description": "A user's residential address which belongs to the profile (they can have many.)",
      "title": "Address"
    },
    "OneTimePasscodeRequest": {
      "type": "object",
      "properties": {
        "emailOrPhone": {
          "type": "string"
        }
      },
      "description": "request",
      "title": "OneTimePasscodeRequest"
    },
    "OneTimePasscodeResponse": {
      "type": "object",
      "description": "response",
      "title": "OneTimePasscodeResponse"
    },
    "OneTimePasscodeVerifyRequest": {
      "type": "object",
      "properties": {
        "emailOrPhone": {
          "type": "string"
        },
        "code": {
          "type": "string"
        }
      },
      "description": "request",
      "title": "OneTimePasscodeVerifyRequest"
    },
    "OneTimePasscodeVerifyResponse": {
      "type": "object",
      "properties": {
        "jwt": {
          "type": "string"
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      },
      "description": "response",
      "title": "OneTimePasscodeVerifyResponse"
    },
    "PlaidConnectBankAccountsRequest": {
      "type": "object",
      "properties": {
        "plaidPublicToken": {
          "type": "string"
        },
        "plaidAccountIds": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "description": "request",
      "title": "PlaidConnectBankAccountsRequest"
    },
    "PlaidConnectBankAccountsResponse": {
      "type": "object",
      "description": "response",
      "title": "PlaidConnectBankAccountsResponse"
    },
    "PlaidCreateLinkTokenRequest": {
      "type": "object",
      "description": "request can be empty since the link token is correlated with the Bearer token passed",
      "title": "PlaidCreateLinkTokenRequest"
    },
    "PlaidCreateLinkTokenResponse": {
      "type": "object",
      "properties": {
        "linkToken": {
          "type": "string"
        }
      },
      "description": "response",
      "title": "PlaidCreateLinkTokenResponse"
    },
    "PricingDataResponse": {
      "type": "object",
      "properties": {
        "rates": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/PricingRate"
          }
        }
      },
      "description": "response",
      "title": "PricingDataResponse"
    },
    "PricingRate": {
      "type": "object",
      "properties": {
        "rate": {
          "type": "object",
          "additionalProperties": {
            "type": "number",
            "format": "float"
          }
        }
      },
      "description": "the pricing rate map; the keys are currency symbols and the values are the rates",
      "title": "PricingRate"
    },
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "createdAt": {
          "type": "string",
          "format": "int64"
        }
      },
      "description": "A user which is shared across customer applications",
      "title": "User"
    },
    "UserFlags": {
      "type": "object",
      "properties": {
        "hasPlaidItems": {
          "type": "boolean"
        },
        "hasWyreAccount": {
          "type": "boolean"
        },
        "hasWyrePaymentMethods": {
          "type": "boolean"
        }
      },
      "description": "hints for the ui",
      "title": "UserFlags"
    },
    "ViewerDataResponse": {
      "type": "object",
      "properties": {
        "user": {
          "$ref": "#/definitions/User"
        },
        "flags": {
          "$ref": "#/definitions/UserFlags"
        }
      },
      "description": "response",
      "title": "ViewerDataResponse"
    },
    "WyreCreateAccountRequest": {
      "type": "object",
      "properties": {
        "individualLegalName": {
          "type": "string"
        },
        "individualResidenceAddress": {
          "$ref": "#/definitions/Address"
        }
      },
      "description": "response",
      "title": "WyreCreateAccountRequest"
    },
    "WyreCreateAccountResponse": {
      "type": "object",
      "description": "response",
      "title": "WyreCreateAccountResponse"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "typeUrl": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
