  "paths": {
    "/flux/auth/one-time-passcode": {
      "post": {
        "summary": "Post email or phone in exchange for a one time passcode",
        "description": "Will cause your email or phone to receive a one time passcode.\nThis can be used in the verify step to obtain a token for login",
        "operationId": "Flux_OneTimePasscode",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/OneTimePasscodeResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/OneTimePasscodeRequest"
            }
          }
        ],
        "tags": [
          "Flux"
        ]
      }
    },
    "/flux/auth/one-time-passcode-verify": {
      "post": {
        "summary": "Post one time passcode in exchange for an access token",
        "description": "The passcode received in either email or phone text message should be provided here in order to obtain on access token",
        "operationId": "Flux_OneTimePasscodeVerify",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/OneTimePasscodeVerifyResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/OneTimePasscodeVerifyRequest"
            }
          }
        ],
        "tags": [
          "Flux"
        ]
      }
    },
    "/flux/pricing-data": {
      "get": {
        "summary": "Get pricing data",
        "description": "Provides pricing data for all markets with rate maps",
        "operationId": "Flux_PricingData",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/PricingDataResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "Flux"
        ]
      }
    },
    "/flux/user-data": {
      "get": {
        "summary": "Get user data",
        "description": "Provides user data associated with the access token",
        "operationId": "Flux_UserData",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/UserDataResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "Flux"
        ]
      }
    },
    "/wyre/add-bank-payment-method": {
      "post": {
        "summary": "Post chosen bank info from plaid in order to create a new ACH pyment method in wyre",
        "description": "requires a plaid processor token which in turn requires a plaid widget interaction where the user selects the account id",
        "operationId": "Flux_WyreAddBankPaymentMethod",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/WyreAddBankPaymentMethodResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/WyreAddBankPaymentMethodRequest"
            }
          }
        ],
        "tags": [
          "Flux"
        ]
      }
    }
  },
