{
  "info": {
    "title": "Flux API",
    "version": "evergreen"
  },
  "securityDefinitions": {
    "Bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  },
  "paths": {
    "/plaid/connect-bank-accounts": {
      "post": {
        "security": [{"Bearer": []}]
      }
    },
    "/plaid/create-link-token": {
      "post": {
        "security": [{"Bearer": []}]
      }
    },
    "/viewer": {
      "get": {
        "security": [{"Bearer": []}]
      }
    },
    "/viewer/email": {
      "put": {
        "security": [{"Bearer": []}]
      }
    },
    "/viewer/phone": {
      "put": {
        "security": [{"Bearer": []}]
      }
    },
    "/viewer/profile": {
      "get": {
        "security": [{"Bearer": []}]
      },
      "patch": {
        "security": [{"Bearer": []}]
      }
    },
    "/wyre/payment-methods": {
      "get": {
        "security": [{"Bearer": []}]
      }
    },
    "/upload": {
      "post": {
        "summary": "Uploads a file and returns a fileId.",
        "operationId": "Flux_UploadFile",
        "tags": [
          "Flux"
        ],
        "security": [
          {
            "Bearer": []
          }
        ],
        "consumes": [
            "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
            {
               "in": "formData",
               "name": "file",
               "type": "file",
               "description": "The file to upload."
            }
         ],
         "responses": {
           "200": {
             "description": "A file upload response object.",
             "schema": {
                "type": "object",
                "properties": {
                   "fileId": {
                      "type": "string",
                      "description": "The file id"
                   },
                   "size": {
                      "type": "integer",
                      "description": "The size of the file just uploaded in bytes"
                   }
                }
             }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
       }
      }
    }
  }
}

