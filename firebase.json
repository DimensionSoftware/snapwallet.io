{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "web_dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "headers": [
    {
      "source": "/widget/dist/init.js",
      "headers": [ {
        "key": "cache-control",
        "value": "max-age=10"
      } ]
    }
  ]
}
